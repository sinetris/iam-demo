{
  "server": {
    "grpc_addr": ":7899",
    "tls_cert_file": "/etc/ssl/notary/tls.crt",
    "tls_key_file": "/etc/ssl/notary/tls.key"
  },
  "logging": {
    "level": "info"
  },
  "storage": {
    "backend": "postgres",
    "db_url": "postgres://postgres:password@notary-postgres:5432/notarysigner?sslmode=disable",
    "default_alias": "defaultalias"
  }
}

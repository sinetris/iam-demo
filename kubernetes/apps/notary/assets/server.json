{
  "server": {
    "http_addr": ":4443",
    "tls_key_file": "/etc/notary/tls/tls.key",
    "tls_cert_file": "/etc/notary/tls/tls.crt"
  },
  "trust_service": {
    "type": "remote",
    "hostname": "notary-signer",
    "port": "7899",
    "tls_ca_file": "/etc/ssl/notary/ca.crt",
    "tls_key_file": "/etc/notary/tls/tls.key",
    "tls_cert_file": "/etc/notary/tls/tls.crt",
    "key_algorithm": "ecdsa"
  },
  "logging": {
    "level": "debug"
  },
  "storage": {
    "backend": "postgres",
    "db_url": "postgres://postgres:password@notary-postgres:5432/notaryserver?sslmode=disable"
  }
}
